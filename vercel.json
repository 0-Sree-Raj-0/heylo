{
    "builds": [{
        "src": "heyloworldproject/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "Python 3.11.4" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "heyloworldproject/wsgi.py"
        }
    ]
}